{
  "name": "lesca-webgl-threejs",
  "version": "1.3.2",
  "description": "It's can build full page webGL easily.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "watch": "babel --watch src/lib --out-dir lib --extensions '.ts,.tsx'",
    "op": "babel src/lib -d lib --copy-files",
    "dev": "concurrently \"npm run watch\" \"npm run start\"",
    "rm": "rm -rf node_modules/ package-lock.json lib/",
    "deploy": "gh-pages -d dist",
    "build": "NODE_ENV=production webpack"
  },
  "keywords": [
    "lesca",
    "threejs",
    "webgl"
  ],
  "license": "MIT",
